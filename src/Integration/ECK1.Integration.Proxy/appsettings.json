{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": "http://localhost:*"
  },
  "Kafka": {
    "BootstrapServers": "set in keyvault",
    "SchemaRegistryUrl": "set in keyvault",
    "User": "set in keyvault",
    "Secret": "set in keyvault",
    "SampleThinEventsTopic": "sample-thin-events",
    "CacheProgressTopic": "cache-progress",
    "GroupIdPrefix": "integration-proxy-group"
  },
  "Doppler": {
    "Token": "set in user secrets",
    "Projects": "eck1-global",
    "Config": "dev",
    "ApiHost": "https://api.doppler.com"
  },
  "Proxy": {
    "Plugin": "set in env",
    "PluginsDir": "set in env"
  },
  "CacheService": {
    "ShortTerm": {
      "Url": "{ set in env }"
    }
  }
}